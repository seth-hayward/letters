<body>

    <div class="page">


        <div id="leftColumn">

            <div id="title">
                <a href="@Url.Content("/")">letters to crushes</a>
            </div>

            <ul id="navigation">
                <li>@Html.ActionLink("send", "send", "Home")</li>
                <li>@Html.ActionLink("more", "more", "Home")</li>
            </ul>

        </div>

        <div id="rightColumn">
            @RenderBody()

        </div>

    </div>
            
    @RenderSection("Footer", required: false)

