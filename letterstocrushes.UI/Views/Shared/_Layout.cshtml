<head>
    <title>letters to crushes</title>

    <link rel="shortcut icon" href="/favicon.ico" type="image/x-icon">
    <link rel="icon" href="/favicon.ico" type="image/x-icon">

    <!-- required for apple icons. !-->

    <link rel="apple-touch-icon" href="@Url.Content("~/Content/apple-touch-icon-precomposed.png")" />
    <link rel="apple-touch-icon" sizes="72x72" href="@Url.Content("~/Content/apple-touch-icon-72x72-precomposed.png")" />
    <link rel="apple-touch-icon" sizes="76x76" href="@Url.Content("~/Content/apple-touch-icon-76x76-precomposed.png")" />
    <link rel="apple-touch-icon" sizes="114x114" href="@Url.Content("~/Content/apple-touch-icon-114x114-precomposed.png")" />
